FUNCTION hanoi :
PARAM v1
PARAM v2
PARAM v3
PARAM v4
IF v1 != #1 GOTO L2
LABEL L1 :
v5 := v2 * #1000000
v6 := v5 + v4
WRITE v6
GOTO L3
LABEL L2 :
v7 := v1 - #1
ARG v7
ARG v2
ARG v4
ARG v3
v8 := CALL hanoi
v9 := v2 * #1000000
v10 := v9 + v4
WRITE v10
v11 := v1 - #1
ARG v11
ARG v3
ARG v2
ARG v4
v12 := CALL hanoi
LABEL L3 :
RETURN #0

FUNCTION main :
v13 := #3
ARG #3
ARG #1
ARG #2
ARG #3

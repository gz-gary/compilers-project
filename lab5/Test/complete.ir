FUNCTION add :
PARAM a
PARAM b
t0 := a + b
RETURN t0
FUNCTION main :
DEC arr 4004
n := #20
i := #2
t2 := #0 * #4
t1 := &arr + t2
*t1 := #1
t4 := #1 * #4
t3 := &arr + t4
*t3 := #1
LABEL label1 :
t5 := #1
IF i <= n GOTO label0
t5 := #0
LABEL label0 :
IF t5 == #0 GOTO label2
t8 := i - #1
t10 := t8 * #4
t9 := &arr + t10
t9 := *t9
t11 := i - #2
t13 := t11 * #4
t12 := &arr + t13
t12 := *t12
ARG t12
ARG t9
t14 := CALL add
t7 := i * #4
t6 := &arr + t7
*t6 := t14
GOTO label1
LABEL label2 :
t16 := n * #4
t15 := &arr + t16
t15 := *t15
WRITE t15
t17 := #0
RETURN #0

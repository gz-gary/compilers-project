FUNCTION main :
READ t0
n := t0
t1 := #1
IF n > #0 GOTO label0
t1 := #0
LABEL label0 :
IF t1 == #0 GOTO label4
WRITE #1
t2 := #0
GOTO label5
LABEL label4 :
t3 := #1
IF n < #0 GOTO label1
t3 := #0
LABEL label1 :
IF t3 == #0 GOTO label2
t4 := #0 - #1
WRITE t4
t5 := #0
GOTO label3
LABEL label2 :
WRITE #0
t6 := #0
LABEL label3 :
LABEL label5 :
RETURN #0

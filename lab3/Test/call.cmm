int s(int x) {
    return x + 1;
}

int p(int x) {
    return x - 1;
}

int add(int x, int y) {
    return x + y;
}

int gcd(int x, int y) {
    if (y == 0) return x;
    else return gcd(y, x - y);
}

int main() {
    int a, b, c, d;
    a = 2;
    b = 10;
    c = s(a);
    write(c);
    c = p(b);
    write(c);
    c = add(a, b);
    write(c);
    c = gcd(2, 3);
    write(c);
    d = read();
    write(d+1);
    return 0;
}
FUNCTION main :
n := #1
t0 := #1
IF n > #0 GOTO label0
t0 := #0
LABEL label0 :
IF t0 == #0 GOTO label4
result := #1
GOTO label5
LABEL label4 :
t1 := #1
IF n < #0 GOTO label1
t1 := #0
LABEL label1 :
IF t1 == #0 GOTO label2
t2 := #0 - #1
result := t2
GOTO label3
LABEL label2 :
result := #0
LABEL label3 :
LABEL label5 :
RETURN result
